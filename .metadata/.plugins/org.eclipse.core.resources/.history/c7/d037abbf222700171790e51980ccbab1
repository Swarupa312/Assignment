
public class conversion 
{
	int id;
	String name;
	public conversion(int n,String name1)
	{
	id=n;
	name=name1;
	}
	public String toString()
	{
		
		return id +" & " +name;
	}
}
class main1 {
	public static void main(String args[])
	{
		int no;
		conversion c1=new conversion(1,"abvc");
		conversion c2=new conversion(1,"abvc");
		System.out.println(c1);
		System.out.println(c1==c2);
		System.out.println(c1.equals(c2));
	}
}
